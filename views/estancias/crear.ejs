<%- include ../layouts/cabecera.ejs %>

<div class="card">
  <div class="card-header">Registrar una nueva estancia</div>
  <div class="card-body">
    <h5 class="card-title">Seleccione la placa el veh√≠culo</h5>
    <form method="post" action="/estancias">
      <div class="form-group">
        <select id="my-select" class="form-control" name="vehiculo">
          <% vehiculos.forEach(vehiculo => { %>
          <option value="<%= vehiculo.id  %> "><%=vehiculo.placa %></option>
          <% }) %>
        </select>
      </div>
      <button class="btn btn-success" type="submit">Guardar</button>
      <a class="btn btn-primary" href="/estancias">Cancelar</a>
    </form>
  </div>
</div>

